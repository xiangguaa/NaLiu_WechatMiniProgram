.pic{
    width: 60rpx;
    height: 60rpx;
    position: relative;
    top: 6rpx;
    
}



.detail_wrp{
    height: 1234rpx;
    border-radius: 10rpx;
    margin: 5rpx;
    overflow: scroll;
    margin-left: 30rpx;
    margin-right: 30rpx;
    
}


.detail{
    background-color: #ffffff;
    border-radius: 15rpx;
    margin: 15rpx;
    padding: 10rpx;
    border-style: solid;
    border-width: 3rpx;
    box-shadow: 0px 2px  3px 2rpx;
    margin-left: 20rpx; 

}


.search_detail{
    background-color: #ffffff;
    border-radius: 15rpx;
    margin: 15rpx;
    padding: 10rpx;
    border-style: solid;
    border-width: 3rpx;
    box-shadow: 0px 2px  3px 2rpx;
    margin-left: 30rpx;
    margin-right: 30rpx;

}

.error_view{
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 70rpx;
    letter-spacing: 10rpx;
}

.stock_name{
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 50rpx;
}
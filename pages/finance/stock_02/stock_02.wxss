.wrp{
    border-radius: 15rpx;
    background-color: #fbfbfb;
}


.title{
    margin-top: 10rpx;
    margin: 5rpx;
    height: 70rpx;
    border-radius: 15rpx;
    display: flex;
    justify-content: center;
  
    align-items: center;
    font-size: 50rpx;
    letter-spacing: 10rpx;
}

.input_wrp{
    height: 70rpx;
    background-color: rgb(236, 233, 233);
    display: flex;
    justify-content: space-between; 
    margin: 7rpx;
    margin-left: 30rpx;
    margin-right: 30rpx;
    border-radius: 30rpx;

    
}

.input{
    /* width: 500rpx; */
    height: 70rpx;
    border-radius: 15rpx;
    text-indent: 10rpx;
    margin-left: 30rpx;
    margin-right: 30rpx;
    
}


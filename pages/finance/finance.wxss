

page{
    height: 100%;
    width: 100%;
}

.main{
    margin: 15rpx;
    height: 95%;
    background-color: #999;
    display: flex;
    /* justify-content: space-between; */
    align-items: center;
    flex-direction: column;
    padding: 10rpx;
}
.title{
    width: 100%;
    height: 100rpx;
    border-radius: 15rpx;
    background-color: rgb(8, 128, 124);
    font-size: 60rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.nav{
    width: 100%;
    height: 30%;
    margin: 15rpx;
    /* margin-top: 20rpx; */
    background-color: rgb(107, 179, 125);
    border-radius: 15rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 50rpx;
    letter-spacing: 10rpx;
    border-radius: 15rpx;
}


page{
    height: 100%;
    width: 100%;
}

.main{
    height: 95%;
    background-color: #fbfbfb;
    display: flex;

    align-items: center;
    flex-direction: column;
    padding: 10rpx;
}
.title{
    width: 100%;
    height: 6%;
    border-radius: 15rpx;
    font-size: 60rpx;
    font-weight: 450;
    margin-bottom: 20rpx;
}

.nav{
    width: 95%;
    height: 30%;
    margin: 15rpx;
    background-color: #ffffff;

    border-radius: 15rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 40rpx;
    border-radius: 15rpx;
    border-style: solid;
    border-width: 0.5rpx;
    box-shadow: 4rpx 2rpx 2rpx 1rpx ;
    border-color: rgb(204, 193, 129);
}


.dapan_img{
    height: 100%;
    width: 70%;
    margin-left: 5rpx;
}
.exchange_img{
    height: 75%;
    width: 70%;
}
.stock_img{
    height: 75%;
    width: 70%;
}

